# Security Configuration
NODE_ENV=production
FORCE_HTTPS=true

# Server Ports
PORT=5000
HTTP_PORT=8080

# Frontend Configuration
FRONTEND_URL=https://localhost:3000

# SSL Configuration
SSL_KEY_PATH=./keys/privatekey.pem
SSL_CERT_PATH=./keys/certificate.pem

# HSTS Configuration
HSTS_MAX_AGE=63072000
HSTS_INCLUDE_SUBDOMAINS=true
HSTS_PRELOAD=true